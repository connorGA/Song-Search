<div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">


  <main class="px-3">
    <h1>Welcome <%= name %> :) </h1>
    <p>Id: <%= id %></p>
    <p>Name: <%= name %></p>
    <p>Email: <%= email %></p>
  
    <form style="margin-bottom: 40px;" method="get" action="/songs/favorites">
      <button id="favButton" type="submit" class="btn btn-primary my-2" >View Favorites</button>
      </form>

    <form style="margin-bottom: 40px;" method="get" action="/songs/dislikes">
      <button id="disButton" type="submit" class="btn btn-primary my-2" >View Dislikes</button>
      </form>

    <form style="margin-bottom: 40px;" method="get" action="/songs/search">
      <button id="profButton" type="submit" class="btn btn-primary my-2" >Search Songs</button>
      </form>

      <form method="get" action="/profile/edit/:<%=id%>">
        <button style="color: white;"class="searchButton" type="submit">Edit Profile</button>
      </form>
    
